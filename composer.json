{
  "name": "divante-ltd/pimcore-document-copier",
  "type": "pimcore-bundle",
  "license": "GPLv3",
  "description": "Pimcore bundle for copying documents between environments",
  "keywords": [
    "pimcore",
    "pimcore-plugin"
  ],
  "homepage": "https://www.divante.com/",
  "authors": [
    {
      "name": "Pascal Dunaj",
      "email": "pdunaj@divante.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "pimcore/pimcore": "^5.6 || >=6.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "symfony/phpunit-bridge": "^4.2",
    "divante-ltd/pimcore-coding-standards": "^0.1",
    "phpmd/phpmd": "^2.6",
    "sebastian/phpcpd": "^4.1",
    "block8/php-docblock-checker": "^1.10",
    "phploc/phploc": "^4.0.0",
    "codeception/codeception": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Divante\\DocumentCopierBundle\\": "src/DocumentCopierBundle/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Divante\\DocumentCopierBundle\\Test\\": "tests/unit/tests/DocumentCopierBundle/"
    },
    "classmap": [
      "app/AppKernel.php"
    ]
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "Divante\\DocumentCopierBundle\\DocumentCopierBundle"
      ]
    }
  }
}
